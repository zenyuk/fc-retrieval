{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "coverageDirectory": "./coverage/",
  "collectCoverage": true,
  "modulePathIgnorePatterns": ["e2e","lib","dist"]
}
