{
  "name": "fc-retrieval-client-js",
  "version": "0.0.1",
  "description": "Filecoin Secondary Retrieval Market Javascript client library.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test-e2e-watch": "jest --config jestconfig.json --watchAll",
    "test-e2e": "jest --config jestconfig.json",
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/consensys/fc-retrieval-client-js.git"
  },
  "keywords": [
    "Filecoin",
    "Retrieval",
    "Client"
  ],
  "bugs": {
    "url": "https://github.com/consensys/fc-retrieval-client-js/issues"
  },
  "homepage": "https://github.com/consensys/fc-retrieval-client-js#readme",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "bn.js": "^5.2.0",
    "@types/qs": "^6.9.6",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/jest": "^22.2.3",
    "axios-mock-adapter": "^1.19.0",
    "codecov": "^3.1.0",
    "jest": "^23.6.0",
    "prettier": "^1.13.2",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^4.3.2"
  }
}
